    "react-native-vector-icons": "^4.0.0",
    "react-navigation": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "lite-server": "^2.3.0",
    "react-native-dotenv": "0.0.3",
    "react-test-renderer": "15.4.2"
